<h1>This are all Projects</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Due date</th>
    <tr>
    <% @projects.each do |project| %>
        <tr>
            <td><%= project.title %></td>
            <td><%= project.description %></td>
            <td><%= project.due_date %></td>
            <td><%= link_to "Show", project_path(project) %></td>
        </tr>
    <% end %>
</table>
